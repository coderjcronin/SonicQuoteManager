<p id="notice"><%= notice %></p>

<h1>Quotes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Site</th>
      <th>Name</th>
      <th>Started</th>
      <th>Updated</th>
      <th>Locked</th>
      <th>Material lines</th>
      <th>Service lines</th>
      <th>Equip lines</th>
      <th>Wire lines</th>
      <th>Source</th>
      <th>Referal</th>
      <th>Sale type</th>
      <th>Taxable</th>
      <th>State</th>
      <th>County</th>
      <th>Account type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.user_id %></td>
        <td><%= quote.site_id %></td>
        <td><%= quote.name %></td>
        <td><%= quote.started %></td>
        <td><%= quote.updated %></td>
        <td><%= quote.locked %></td>
        <td><%= quote.material_lines %></td>
        <td><%= quote.service_lines %></td>
        <td><%= quote.equip_lines %></td>
        <td><%= quote.wire_lines %></td>
        <td><%= quote.source %></td>
        <td><%= quote.referal %></td>
        <td><%= quote.sale_type %></td>
        <td><%= quote.taxable %></td>
        <td><%= quote.state_id %></td>
        <td><%= quote.county_id %></td>
        <td><%= quote.account_type %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
